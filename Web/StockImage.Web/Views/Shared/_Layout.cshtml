<!DOCTYPE html>
<html lang="en">
<head>
    
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Stoyan's Portfolio</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/css/bootstrap.min.css">
    <link rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.0.0-beta/css/bootstrap.min.css'>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    <link rel="stylesheet" href="~/css/style.css">

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Alex+Brush&display=swap" rel="stylesheet">

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@200&display=swap" rel="stylesheet">


</head>
<body>
    <div class="container-fluid">

    <header>
        <nav class="navbar navbar-dark bg-dark navbar-expand-sm">
            <div class="container">
                <div class="collapse navbar-collapse"></div>

                <div class="collapse navbar-collapse">
                    <ul class="navbar-nav mr-auto">

                        <li class="navbar-brand text-success">
                            <a class="nav-link logo"
                               href="/Home/Index">
                                Stoyan Kyumyurdjiev
                            </a>
                        </li>

                    </ul>
                </div>
            </div>
        </nav>
    </header>


    <div class="container-main">
        <main role="main">
            @RenderBody()
        </main>
    </div>


    <hr />

    <div class="footer-basic">
        <footer>
            <div class="social">
                <a href="https://www.instagram.com/" target="_blank"><i class="icon ion-social-instagram"></i></a>
                <a href="https://github.com/StoyanKYM" target="_blank"><i class="icon ion-social-github" target="_blank"></i></a>
                <a href="https://www.linkedin.com/in/stoyan-kyumyurdzhiev/" target="_blank"><i class="icon ion-social-linkedin" target="_blank"></i></a>
                <a href="https://www.facebook.com/" target="_blank"><i class="icon ion-social-facebook" target="_blank"></i></a>
            </div>
            <ul class="list-inline">
                <li class="list-inline-item"><a href="/Home/Index" target="_blank">Home</a></li>
                <li class="list-inline-item"><a href="/Image/All" target="_blank">Gallery</a></li>
                <li class="list-inline-item"><a href="/Home/About" target="_blank">About</a></li>
                <li class="list-inline-item"><a href="/Comment/Add" target="_blank">Feedback</a></li>
            </ul>
            <p class="copyright">StoyanKyumyurdjiev © 2021</p>
        </footer>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
    </div>
</body>
</html>
